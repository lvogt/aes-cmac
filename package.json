{
  "name": "aes-cmac",
  "version": "1.0.3",
  "description": "AES-CMAC implementation in typescript",
  "types": "lib/import/AesCmac.d.ts",
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "watch": "tsc -w",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.ts'",
    "prepare": "npm run build",
    "example": "ts-node examples/example.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rosek86/aes-cmac.git"
  },
  "keywords": [
    "aes-cmac"
  ],
  "author": "Krzysztof Rosinski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rosek86/aes-cmac/issues"
  },
  "homepage": "https://github.com/rosek86/aes-cmac/#readme",
  "files": [
    "lib/**/*"
  ],
  "exports": {
    "import": "./lib/import/AesCmac.js",
    "require": "./lib/require/AesCmac.js"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.38",
    "mocha": "^10.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "one-webcrypto": "^1.0.3"
  }
}
